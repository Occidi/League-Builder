{
  "name": "leaguebuilder",
  "version": "1.0.0",
  "description": "League builder : used to share popular league of legends items builds using built in Riot Games recommended items system",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "./node_modules/.bin/babel src/server --out-dir ./dist",
    "build-client": "./node_modules/.bin/babel src/client  --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test": "jest"
  },
  "author": "Gwendal Jeanson",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-cli": "^7.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0"
  }
}
