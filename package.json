{
  "name": "leaguebuilder",
  "version": "1.0.0",
  "description": "League builder : used to share popular league of legends items builds using built in Riot Games recommended items system",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-server && npm run build-client",
    "build-babel-server": "./node_modules/.bin/babel src/server --out-dir ./dist",
    "build-babel-client": "./node_modules/.bin/babel src/client  --copy-files --out-dir ./dist/public",
    "start": "node ./dist/server.js",
    "test": "jest",
    "clean": "rimraf dist node_modules"
  },
  "author": "Gwendal Jeanson",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
